{
    "name": "laxamar/gpiosysv",
    "description": "A library for accessing the GPIO pins on a Raspberry Pi without root for clients using SysV message queue and client/server",
    "keywords": ["gpio", "pi", "raspberry", "pin", "pins", "sysv", "no-root"],
    "license": "MIT",
    "authors": [
        {
            "name": "Jacques Amar",
            "email": "jx.gpiosysv@amar.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Amar\\GPIOSysV\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PiPHP\\Test\\GPIO\\": "test/"
        }
    },
    "require": {
        "php": "^7.4||^8.0",
        "piphp/gpio": "*",
        "ext-sysvmsg": "*",
        "ext-pcntl": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3"
    },
    "scripts": {
        "test": [
            "php vendor/bin/phpunit"
        ]
    }
}
